  "homepage":         "http://patternlab.io",
  "license":          "MIT",
  "type":             "patternlab-plugin",
  "authors": [
    {
      "name":         "Dave Olsen",
      "email":        "dmolsen@gmail.com",
      "homepage":     "http://dmolsen.com",
      "role":         "Lead Developer"
    }
  ],
  "support": {
    "issues":         "https://github.com/pattern-lab/mustachehelper-php-verbatim/issues",
    "wiki":           "http://patternlab.io/docs/",
